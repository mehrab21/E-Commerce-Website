
@{
    ViewData["Title"] = "feedback";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}

@model List<Feedback>
<div class="container-fluid px-4">
	<h2 class="mt-4 text-center text-uppercase" style="letter-spacing:4px">
		all Feedback
	</h2>
</div>
<div class="row">
	<div class="col-md-12">
		<table class="table table-bordered table-striped text-center mt-2">
		<thead>
			<tr>
				
				<th>User Name</th>
				<th> User Message</th>
				<th>Option</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var cat in Model)
			{
				<tr>
					
					<td>@cat.user_name </td>
					<td>@cat.user_message</td>
					<td>
							<a href="@Url.Action("DeleteParmissionFeed","admin",new{id=cat.Feedback_Id})" class="btn btn-danger">Delete</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>
</div>

